 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"findMax", "[", 
     RowBox[{"list_", ",", "n_"}], "]"}], ":=", 
    RowBox[{"Module", "[", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"a", ",", "i", ",", "max", ",", "pos"}], "}"}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"a", "=", "list"}], ";", "\[IndentingNewLine]", 
       RowBox[{"max", "=", "0"}], ";", "\[IndentingNewLine]", 
