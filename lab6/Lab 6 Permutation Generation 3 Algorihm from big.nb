Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"BottomUp", "[", 
    RowBox[{"n_", ",", "rd_", ",", "list_"}], "]"}], ":=", 
   RowBox[{"Module", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"nextL", ",", "temp", ",", "i", ",", "j", ",", "k"}], "}"}], 
     ",", "\[IndentingNewLine]", 
     RowBox[{
