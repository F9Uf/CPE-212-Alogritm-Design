Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"lexicographic", "[", 
    RowBox[{"a_", ",", "n_"}], "]"}], ":=", 
   RowBox[{"Module", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "list", ",", "i", ",", "j", ",", "largestI", ",", "largestJ", ",", 
       "endList"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"list", "=", "a"}], ";", "\[IndentingNewLine]", 
      RowBox[{"i", "=", "0"}], ";", "\[IndentingNewLine]", 
      RowBox[{"While", "[", 
       RowBox[{"True", ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"Print", "[", "list", "]"}], ";", "\[IndentingNewLine]", 
         RowBox[{"largestI", "=", "0"}], ";", "\[IndentingNewLine]", 
         RowBox[{"For", "[", 
          RowBox[{
           RowBox[{"i", "=", "1"}], ",", 
           RowBox[{"i", "\[LessEqual]", 
            RowBox[{"n", "-", "1"}]}], ",", 
           RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"If", "[", 
             RowBox[{
              RowBox[{
               RowBox[{
               "list", "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}],
                "<", 
               RowBox[{"list", "\[LeftDoubleBracket]", 
                RowBox[{"i", "+", "1"}], "\[RightDoubleBracket]"}]}], ",", 
              RowBox[{"largestI", "=", "i"}]}], "]"}], ";"}]}], 
          "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"largestI", "\[Equal]", "0"}], ",", 
           RowBox[{"Break", "[", "]"}]}], "]"}], ";", "\[IndentingNewLine]", 
         "\[IndentingNewLine]", 
         RowBox[{"largestJ", "=", "0"}], ";", "\[IndentingNewLine]", 
         RowBox[{"For", "[", 
          RowBox[{
           RowBox[{"j", "=", "1"}], ",", 
           RowBox[{"j", "\[LessEqual]", "n"}], ",", 
           RowBox[{"j", "++"}], ",", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"If", "[", 
             RowBox[{
              RowBox[{
               RowBox[{
               "list", "\[LeftDoubleBracket]", "j", "\[RightDoubleBracket]"}],
                ">", 
               RowBox[{
               "list", "\[LeftDoubleBracket]", "largestI", 
                "\[RightDoubleBracket]"}]}], ",", 
              RowBox[{"largestJ", "=", "j"}]}], "]"}], ";"}]}], 
          "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{
            RowBox[{
            "list", "\[LeftDoubleBracket]", "largestI", 
             "\[RightDoubleBracket]"}], ",", 
            RowBox[{
            "list", "\[LeftDoubleBracket]", "largestJ", 
             "\[RightDoubleBracket]"}]}], "}"}], "=", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{
            "list", "\[LeftDoubleBracket]", "largestJ", 
             "\[RightDoubleBracket]"}], ",", 
            RowBox[{
            "list", "\[LeftDoubleBracket]", "largestI", 
             "\[RightDoubleBracket]"}]}], "}"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"list", "=", 
          RowBox[{"Join", "[", 
           RowBox[{
            RowBox[{"Take", "[", 
             RowBox[{"list", ",", "largestI"}], "]"}], ",", 
            RowBox[{"Reverse", "[", 
             RowBox[{"Drop", "[", 
              RowBox[{"list", ",", "largestI"}], "]"}], "]"}]}], "]"}]}], 
         ";"}]}], "\[IndentingNewLine]", "\[IndentingNewLine]", "]"}], 
      ";"}]}], "\[IndentingNewLine]", "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"n", "=", "4"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"list", "=", 
   RowBox[{"Range", "[", "n", "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"lexicographic", "[", 
   RowBox[{"list", ",", "n"}], "]"}], ";"}]}], "Input",
