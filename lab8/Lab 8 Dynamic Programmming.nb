Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"WarshallAlgorithm", "[", "A_", "]"}], ":=", 
   RowBox[{"Module", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"k", ",", "i", ",", "j", ",", "n", ",", "R"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"R", "=", 
       RowBox[{"{", "A", "}"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"n", "=", 
       RowBox[{"Length", "[", "A", "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"k", "=", "1"}], ",", 
        RowBox[{"k", "\[LessEqual]", "n"}], ",", 
        RowBox[{"k", "++"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"AppendTo", "[", 
          RowBox[{"R", ",", 
           RowBox[{
           "R", "\[LeftDoubleBracket]", "k", "\[RightDoubleBracket]"}]}], 
          "]"}], ";", "\[IndentingNewLine]", 
         RowBox[{"For", "[", 
          RowBox[{
           RowBox[{"i", "=", "1"}], ",", 
           RowBox[{"i", "\[LessEqual]", "n"}], ",", 
           RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"For", "[", 
             RowBox[{
              RowBox[{"j", "=", "1"}], ",", 
              RowBox[{"j", "\[LessEqual]", "n"}], ",", 
              RowBox[{"j", "++"}], ",", "\[IndentingNewLine]", 
              RowBox[{
               RowBox[{
                RowBox[{
                 RowBox[{
                  RowBox[{"R", "\[LeftDoubleBracket]", 
                   RowBox[{"k", "+", "1"}], "\[RightDoubleBracket]"}], 
                  "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}], 
                 "\[LeftDoubleBracket]", "j", "\[RightDoubleBracket]"}], "=", 
                RowBox[{"BitOr", "[", 
                 RowBox[{
                  RowBox[{
                   RowBox[{
                    RowBox[{
                    "R", "\[LeftDoubleBracket]", "k", 
                    "\[RightDoubleBracket]"}], "\[LeftDoubleBracket]", "i", 
                    "\[RightDoubleBracket]"}], "\[LeftDoubleBracket]", "j", 
                   "\[RightDoubleBracket]"}], ",", 
                  RowBox[{"BitAnd", "[", 
                   RowBox[{
                    RowBox[{
                    RowBox[{
                    RowBox[{
                    "R", "\[LeftDoubleBracket]", "k", 
                    "\[RightDoubleBracket]"}], "\[LeftDoubleBracket]", "i", 
                    "\[RightDoubleBracket]"}], "\[LeftDoubleBracket]", "k", 
                    "\[RightDoubleBracket]"}], ",", 
                    RowBox[{
                    RowBox[{
                    RowBox[{
                    "R", "\[LeftDoubleBracket]", "k", 
                    "\[RightDoubleBracket]"}], "\[LeftDoubleBracket]", "k", 
                    "\[RightDoubleBracket]"}], "\[LeftDoubleBracket]", "j", 
                    "\[RightDoubleBracket]"}]}], "]"}]}], "]"}]}], ";"}]}], 
             "\[IndentingNewLine]", "]"}], ";"}]}], "\[IndentingNewLine]", 
          "]"}], ";"}]}], "\[IndentingNewLine]", "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Return", "[", 
       RowBox[{"R", "\[LeftDoubleBracket]", 
        RowBox[{"n", "+", "1"}], "\[RightDoubleBracket]"}], "]"}], ";"}]}], 
    "\[IndentingNewLine]", "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.750544338897165*^9, 3.750544358334799*^9}, 
   3.7505445270490727`*^9, {3.750544581051162*^9, 3.7505446329421296`*^9}, 
   3.7505446991379156`*^9}],

